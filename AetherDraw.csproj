<Project Sdk="Dalamud.NET.Sdk/13.0.0">

    <PropertyGroup>
        <AssemblyName>AetherDraw</AssemblyName>
        <RootNamespace>AetherDraw</RootNamespace>
        <AssemblyVersion>2.3.0.4</AssemblyVersion>
        <TargetFramework>net9.0-windows</TargetFramework>
        <Platforms>x64</Platforms>
        <PlatformTarget>x64</PlatformTarget>
        <LangVersion>latest</LangVersion>
        <Description>A whiteboard raid planner plugin to draw encounter strategies, with live collaboration! Supports raidplan url imports</Description>
        <PackageProjectUrl>https://github.com/rail2025/AetherDraw/</PackageProjectUrl>
        <Authors>rail</Authors>
        <Version>2.3.0.4</Version>
        <NoWarn>CA1416</NoWarn>
    </PropertyGroup>

    <ItemGroup>
      <EmbeddedResource Include="PluginImages\toolbar\A.png" />
      <EmbeddedResource Include="PluginImages\toolbar\B.png" />
      <EmbeddedResource Include="PluginImages\toolbar\C.png" />
      <EmbeddedResource Include="PluginImages\toolbar\D.png" />
    </ItemGroup>

